{
  "name": "todolist",
  "version": "0.0.1",
  "description": "Nhắc hạn đúng hẹn",
  "productName": "Todolist App",
  "author": "Lo Van Long <lovanlong@amycos.vn>",
  "private": true,
  "scripts": {
    "dev": "npx quasar dev",
    "build": "npx quasar build -m pwa",
    "zip": "icongenie generate -m pwa -i longwebstudio.png && rm -f wp-pwa.zip && rm -rf app && npx quasar build -m pwa && mv dist/pwa app && zip -r wp-pwa.zip ./app",
    "push": "icongenie generate -m pwa -i longwebstudio.png && rm -rf push/docs && npx quasar build -m pwa && mv dist/pwa push/docs && git add . && git commit -m build && git push && cd push && git add . && git commit -m build && git push",
    "start": "node server.js",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "apollo-boost": "^0.4.9",
    "apollo-link": "^1.2.14",
    "axios": "^0.21.1",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.6.5",
    "express": "^4.17.3",
    "firebase": "^9.22.2",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "moment": "^2.29.1",
    "quasar": "^2.6.0",
    "serve-static": "^1.15.0",
    "vue": "^3.0.0",
    "vue-apollo": "^3.1.0",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app-webpack": "^3.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "prettier": "^2.5.1",
    "workbox-webpack-plugin": "^6.5.2"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
